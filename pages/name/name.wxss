.container {
  display: flex;
  width: 100%;
  height: 100%;
  justify-content: flex-start;
  align-items: center;
}

.first-name-box {
  display: flex;
  margin-left: 20rpx;
  margin-right: 20rpx;
  width: 80%;
  height: 100rpx;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  border: 1px solid #EAEAEA;
  border-radius: 5px;
}

.name-text {
  display: flex;
  width: 50rpx;
  margin-left: 20rpx;
}

.name-input{
    display: flex;
    flex: 1;
}

.radio-box{
    display: flex;
    justify-content: flex-start;
    align-items: center;
    width: 80%;
    height: 100rpx;
}

.button-box{
    width: 80%;
    height: 100rpx;
}